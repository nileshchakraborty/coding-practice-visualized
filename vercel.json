{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/execute",
            "destination": "/api/execute.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/execute.py": {
            "maxDuration": 30,
            "memory": 1024
        },
        "api/index.ts": {
            "maxDuration": 15,
            "memory": 1024,
            "includeFiles": [
                "api/data/**"
            ]
        }
    }
}